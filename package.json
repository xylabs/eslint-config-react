{
  "name": "@xylabs/eslint-config-react",
  "homepage": "https://github.com/xylabs/eslint-config-react.git",
  "bugs": {
    "url": "https://github.com/xylabs/eslint-config-react/issues",
    "email": "support@xylabs.com"
  },
  "description": "ESLint Config used throughout XY Labs TypeScript/JavaScript React libraries and projects",
  "author": {
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com",
    "email": "support@xylabs.com"
  },
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/eslint-config-react.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xylabs",
    "utility",
    "typescript",
    "eslint"
  ],
  "version": "2.1.7",
  "main": "./dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "deploy": "yarn version --patch && yarn publish ./",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install"
  },
  "dependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-syntax-flow": "^7.17.12",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@xylabs/eslint-config": "^2.3.28",
    "eslint": "^8.18.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "peerDependencies": {
    "react": "^17"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@xylabs/ts-scripts": "^2.14.1",
    "@xylabs/tsconfig": "^1.0.14",
    "react": "^18.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.0"
}
