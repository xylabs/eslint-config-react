{
  "name": "@xylabs/eslint-config-react",
  "homepage": "https://github.com/xylabs/eslint-config-react.git",
  "bugs": {
    "url": "https://github.com/xylabs/eslint-config-react/issues",
    "email": "support@xylabs.com"
  },
  "description": "ESLint Config used throughout XY Labs TypeScript/JavaScript React libraries and projects",
  "author": {
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com",
    "email": "support@xylabs.com"
  },
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/eslint-config-react.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xylabs",
    "utility",
    "typescript",
    "eslint"
  ],
  "version": "2.1.0",
  "main": "./dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "deploy": "yarn version --patch && yarn publish ./",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@rushstack/eslint-patch": "^1.1.0",
    "@xylabs/eslint-config": "^2.2.10",
    "eslint": "^8",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2"
  },
  "peerDependencies": {
    "react": "^17"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@xylabs/tsconfig": "^1.0.8",
    "eslint": "^8",
    "prettier": "^2",
    "react": "^17",
    "tslib": "^2.3.1",
    "typescript": "^4"
  }
